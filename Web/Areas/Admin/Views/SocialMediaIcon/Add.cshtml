@model SocialMediaIconsAddDto
@{
    Layout = "_Layout";
}
<form asp-action="Add" asp-controller="SocialMediaIcon" method="post">
    <div class="form-group mb-3">
        <label>Ikon Bilgisi: </label><span asp-validation-for="@Model.Icon" class="text-danger"></span>
        <input asp-for="@Model.Icon" class="form-control" />
    </div>
    <div class="form-group mb-3">
        <label>Link Bilgisi: </label><span asp-validation-for="@Model.Link" class="text-danger"></span>
        <input asp-for="@Model.Link" class="form-control" />
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-outline-primary">Kaydet</button>
    </div>
</form>